	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6b7279"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.parameter",
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#b690ff"
			}
		},
		{
			"name": "Colors",
			"scope": ["constant.other.color"],
			"settings": {
				"foreground": "#f0c07d"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword.control",
				"keyword",
				"storage.type",
				"storage.modifier",
				"storage.type.function.php",
				"meta.function.php",
				"support.function.construct.output.php",
				"meta.embedded.block.php",
				"entity.name.tag",
				"meta.tag.sgml",
